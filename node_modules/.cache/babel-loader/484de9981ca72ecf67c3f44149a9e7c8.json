L,CAAU,IAAV,CAAe,KAAf,EAAsB,KAAtB,IAA+B,CAAC,KAAK,IAAL,CAAU,IAAV,CAAe,KAAf,EAAsB,KAAtB,CAAhC;AACD,KAPM;AAQP,IAAA,OARO,mBAQC,KARD,EAQQ,KARR,EAQe;AACpB,WAAK,IAAL,CAAU,IAAV,CAAe,KAAf,EAAsB,KAAtB,IAA+B,CAAC,KAAK,IAAL,CAAU,IAAV,CAAe,KAAf,EAAsB,KAAtB,CAAhC;AACA,MAAA,KAAI,CACD,GADH,CACO,wBADP,EACiC,KAAK,IAAL,CAAU,IAAV,CAAe,KAAf,CADjC,EACwD;AACpD,QAAA,OAAO,EAAE;AACP,0BAAgB;AADT;AAD2C,OADxD,EAMG,IANH,CAMQ,UAAC,GAAD;AAAA,eAAS,OAAO,CAAC,GAAR,CAAY,GAAZ,CAAT;AAAA,OANR,EAOG,KAPH,CAOS,UAAC,CAAD;AAAA,eAAO,OAAO,CAAC,GAAR,CAAY,CAAZ,CAAP;AAAA,OAPT;AAQD;AAlBM;AAjBI,CAAf","sourcesContent":["<template>\r\n  <div>\r\n    <div class=\"container mt-3 row row-cols-1 g-2\" v-if=\"!isEmpty\">\r\n      <div\r\n        class=\"card text-black ms-3 col\"\r\n        style=\"max-width: 18rem;\"\r\n        v-for=\"(val, index) in data.data\"\r\n        :key=\"index\"\r\n      >\r\n        <div class=\"mb-3\" v-show=\"val.__v\">\r\n          <label class=\"form-label\">Name</label>\r\n          <input type=\"email\" class=\"form-control\" v-model=\"val.name\" />\r\n        </div>\r\n        <div class=\"mb-3\" v-show=\"val.__v\">\r\n          <label class=\"form-label\">Age</label>\r\n          <input type=\"text\" class=\"form-control\" v-model=\"val.age\" />\r\n        </div>\r\n        <div class=\"mb-3\" v-show=\"val.__v\">\r\n          <label class=\"form-label\">Email</label>\r\n          <input type=\"email\" class=\"form-control\" v-model=\"val.email\" />\r\n        </div>\r\n        <div class=\"card-header\" v-show=\"!val.__v\">\r\n          Name: <span style=\"color: Red\" v-show=\"!val.__v\">{{ val.name }}</span>\r\n        </div>\r\n        <div class=\"card-body\">\r\n          <p class=\"card-title\" v-show=\"!val.__v\">Age: {{ val.age }}</p>\r\n          <p v-show=\"!val.__v\">Email: {{ val.email }}</p>\r\n          <button\r\n            class=\"btn btn-success me-3\"\r\n            @click=\"editCard($event, index)\"\r\n            v-show=\"!val.__v\"\r\n          >\r\n            Редактировать\r\n          </button>\r\n          <button\r\n            class=\"btn btn-success\"\r\n            @click=\"putCard($event, index)\"\r\n            v-show=\"val.__v\"\r\n          >\r\n            OK\r\n          </button>\r\n          <button\r\n            class=\"btn btn-danger\"\r\n            v-show=\"!val.__v\"\r\n            @click=\"deleteCard($event, val, index)\"\r\n          >\r\n            Удалить\r\n          </button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"spinner-border\" role=\"status\" v-else>\r\n      <span class=\"visually-hidden\">Loading...</span>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\nexport default {\r\n  data() {\r\n    return {\r\n      data: {},\r\n    }\r\n  },\r\n  mounted() {\r\n    axios\r\n      .get('http://localhost:3000')\r\n      .then((res) => (this.data = res))\r\n      .catch((err) => console.log(err))\r\n  },\r\n  computed: {\r\n    isEmpty() {\r\n      return Object.keys(this.data).length == 0\r\n    },\r\n  },\r\n  methods: {\r\n    deleteCard(e, data, i) {\r\n      axios.delete(`http://localhost:3000/` + data['_id'])\r\n      this.data.data.splice(i, 1)\r\n    },\r\n    editCard(event, index) {\r\n      this.data.data[index]['__v'] = !this.data.data[index]['__v']\r\n    },\r\n    putCard(event, index) {\r\n      this.data.data[index]['__v'] = !this.data.data[index]['__v']\r\n      axios\r\n        .put('http://localhost:3000/', this.data.data[index], {\r\n          headers: {\r\n            'Content-Type': 'application/json',\r\n          },\r\n        })\r\n        .then((res) => console.log(res))\r\n        .catch((e) => console.log(e))\r\n    },\r\n  },\r\n}\r\n</script>\r\n"],"sourceRoot":""}]}